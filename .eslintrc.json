{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": "off",
        "max-len": "off",
        "indent": "off",
        "object-curly-spacing": "off",
        "require-jsdoc": "off",
        "prefer-promise-reject-errors": "off"
    }
}
